{
  "name": "azujuuuuuun.github.io",
  "private": true,
  "description": "azujuuuuuun.github.io",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "lint:js": "yarn run eslint src/**/*.js",
    "lint": "yarn run lint:js",
    "type-check": "yarn run tsc --noEmit",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "gatsby": "2.3.4",
    "gatsby-plugin-google-analytics": "2.0.18",
    "gatsby-plugin-manifest": "2.0.26",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.11",
    "gatsby-plugin-sharp": "2.0.32",
    "gatsby-plugin-typescript": "2.0.11",
    "gatsby-plugin-typography": "2.2.10",
    "gatsby-remark-images": "3.0.10",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.3.8",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-github": "0.16.19"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
    "@types/typography": "^0.16.2",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  }
}
