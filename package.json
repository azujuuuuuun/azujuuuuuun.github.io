{
  "name": "azujuuuuuun.github.io",
  "private": true,
  "description": "azujuuuuuun.github.io",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "lint:ts": "yarn run eslint . --ext .ts --ext .tsx",
    "lint": "yarn run lint:ts",
    "type-check": "yarn run tsc --noEmit",
    "test": "jest",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "gatsby": "2.21.19",
    "gatsby-plugin-google-analytics": "2.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-typescript": "2.4.2",
    "gatsby-plugin-typography": "2.5.1",
    "gatsby-remark-images": "3.3.1",
    "gatsby-remark-prismjs": "3.5.1",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "prismjs": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-github": "0.16.19"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/react-helmet": "5.0.8",
    "@types/react-test-renderer": "16.8.2",
    "@types/typography": "0.16.2",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-preset-gatsby": "0.4.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "gh-pages": "2.0.1",
    "husky": "2.6.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.2",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check && yarn run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run eslint --fix",
      "git add"
    ]
  }
}
