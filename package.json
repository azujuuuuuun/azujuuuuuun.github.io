{
  "name": "azujuuuuuun.github.io",
  "private": true,
  "description": "azujuuuuuun.github.io",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "lint:ts": "yarn run eslint src/**/*.{ts,tsx}",
    "lint": "yarn run lint:ts",
    "type-check": "yarn run tsc --noEmit",
    "test": "jest",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "gatsby": "2.3.32",
    "gatsby-plugin-google-analytics": "2.0.18",
    "gatsby-plugin-manifest": "2.0.29",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-typescript": "2.0.13",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-remark-images": "3.0.11",
    "gatsby-source-filesystem": "2.0.32",
    "gatsby-transformer-remark": "2.3.12",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-github": "0.16.19"
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/react-helmet": "5.0.8",
    "@types/react-test-renderer": "16.8.1",
    "@types/typography": "0.16.2",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-preset-gatsby": "0.1.11",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check && yarn run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run eslint --fix",
      "git add"
    ]
  }
}
