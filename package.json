{
  "name": "azujuuuuuun.github.io",
  "version": "0.1.0",
  "description": "azujuuuuuun.github.io",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:script": "biome lint src/",
    "lint:style": "stylelint '**/*.scss'",
    "lint": "pnpm run lint:script && pnpm run lint:style",
    "format": "biome format . --write",
    "test": "vitest run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.651.1",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "next": "14.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@testing-library/jest-dom": "6.3.0",
    "@testing-library/react": "16.0.1",
    "@types/node": "20.16.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.2.1",
    "husky": "9.1.5",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.10",
    "sass": "1.78.0",
    "stylelint": "16.9.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vitest": "1.6.0"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --apply --no-errors-on-unmatched",
      "biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
      "biome check --apply-unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --apply --no-errors-on-unmatched"
    ]
  },
  "volta": {
    "node": "20.17.0"
  }
}
